<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>@(ViewBag.Title + '-' + SiteConfiguration.Value.Title)</title>
    <meta name="keywords" content="@ViewBag.Keywords" />
    <meta name="description" content="@Html.Raw(ViewBag.Description)" />
    <meta name="author" content="">
    <link rel="icon" href="@Url.Content("/favicon.ico")">
    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="@Url.Content("/lib/font-awesome/css/font-awesome.min.css")" asp-append-version="true" />
    <link rel="stylesheet" href="@Url.Content("/css/mofang.mobilesite.dashboard.min.css")" asp-append-version="true" />
    @if (IsSectionDefined("Header"))
    {
        @await RenderSectionAsync("Header")
    }
    @if (IsSectionDefined("Css"))
    {
        @await RenderSectionAsync("Css")
    }
    <script src="@Url.Content("/js/mofang.mobilesite.min.js")" asp-append-version="true"></script>
    <script src="@Url.Content("/js/mofang.mobilesite.dashboard.min.js")" asp-append-version="true"></script>
    <script src="@Url.Content("/js/particles/particles.js")" asp-append-version="true"></script>
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="">
    @RenderBody()
    <script>
        var resizefunc = [];
    </script>
    @if (IsSectionDefined("Scripts"))
    {
        @await RenderSectionAsync("Scripts")
    }
</body>
</html>
<script>
    @*particlesJS.load('particles-js', '@Url.Content("/js/particles/particles.json")', function () {
    });*@
</script>
